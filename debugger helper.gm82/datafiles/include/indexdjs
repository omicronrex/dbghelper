            const readCollision = function (data) {
                if (data.readUInt32LE() != VERSION_COLLISION)
                    throw new Error("Sprite collision version is incorrect");
                const width = data.readUInt32LE();
                const height = data.readUInt32LE();
                const bBoxLeft = data.readUInt32LE();
                const bBoxRight = data.readUInt32LE();
                const bBoxTop = data.readUInt32LE();
                const bBoxBottom = data.readUInt32LE();
                const maskSize = width * height;
                const mask = [...data.readBuffer(maskSize * 4)].map(n => n != 0);
                return {
                    width: width,
                    height: height,
                    bBoxLeft: bBoxLeft,
                    bBoxRight: bBoxRight,
                    bBoxTop: bBoxTop,
                    bBoxBottom: bBoxBottom,
                    data: mask,
                };
            };
            sprite.perFrameColliders = data.readUInt32LE() != 0;
            if (sprite.perFrameColliders) {
                sprite.colliders = new Array(frameCount);
                for (let i; i < frameCount; ++i)
                    readCollision(data);
            }
            else {
                sprite.colliders = [readCollision(data)];
            }
        }
        return sprite;
    }
    serialize(data) {
    }
}

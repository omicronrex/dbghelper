        for (let i = 0; i < 256; ++i)
            reverseTable[swapTable[i]] = i;
        const length = exe.readUInt32LE();
        const pos = exe.readOffset;
        for (let i = length + pos; i > pos; --i) {
            exe.internalBuffer[i - 1] = utils_1.Utils.overflowingSub(reverseTable[exe.internalBuffer[i - 1]], utils_1.Utils.overflowingAdd(exe.internalBuffer[i - 2], utils_1.Utils.overflowingSub(i, pos + 1, 8)[0], 8)[0], 8)[0];
        }
        for (let i = pos + length - 1; i >= pos; --i) {
            const b = Math.max(i - swapTable[((i - pos) & 0xFF) >>> 0], pos);
            const a = exe.internalBuffer[i];
            exe.internalBuffer[i] = exe.internalBuffer[b];
            exe.internalBuffer[b] = a;
        }
        return uniqueKey;
    }
    static encrypt(exe) {
        const reverseTable = new Array(256).fill(0);
        const garbageSize1 = exe.readUInt32LE() * 4;
        const garbageSize2 = exe.readUInt32LE() * 4;
        exe.readOffset += garbageSize1;
        const swapTable = [...exe.readBuffer(256)];
        exe.readOffset += garbageSize2;
        for (let i = 0; i < 256; ++i)
            reverseTable[swapTable[i]] = i;
        let length = exe.readUInt32LE();
        const pos = exe.readOffset;
        length = exe.length - pos;
        exe.writeOffset = pos - 4;
        exe.writeUInt32LE(length);
        for (let i = pos; i < pos + length; ++i) {
            const b = Math.max(i - swapTable[((i - pos) & 0xFF) >>> 0], pos);
            const a = exe.internalBuffer[i];
            exe.internalBuffer[i] = exe.internalBuffer[b];
            exe.internalBuffer[b] = a;
        }
        for (let i = pos; i < pos + length; ++i) {
            exe.internalBuffer[i] = swapTable[utils_1.Utils.overflowingAdd(exe.internalBuffer[i], utils_1.Utils.overflowingAdd(i - pos, exe.internalBuffer[i - 1], 8)[0], 8)[0]];
        }
    }
}
exports.GM80 = GM80;



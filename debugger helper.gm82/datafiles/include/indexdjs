                for (let k = 0; k < ARG_MAX; ++k)
                    argTypes[k] = FunctionValueKindFrom(exe.readUInt32LE());
                const returnType = FunctionValueKindFrom(exe.readUInt32LE());
                functions[j] = {
                    name: name,
                    externalName: externalName,
                    convention: convention,
                    id: id,
                    argCount: argCount,
                    argTypes: argTypes,
                    returnType: returnType,
                };

        let n2 = 0x4650;
        if (this.iter !== null) {
            n1 = this.iterNext();
            n2 = this.iterNext();
        }
        this.seed1 = ((0xFFFF & this.seed1) >>> 0) * n1 + (this.seed1 >>> 16);
        this.seed2 = ((0xFFFF & this.seed2) >>> 0) * n2 + (this.seed2 >>> 16);
        const result = ((this.seed1 << 16) >>> 0) + ((this.seed2 & 0xFFFF) >>> 0);
        return result;
    }
    iterNext() {
        const result = this.iter[this.index++];
        if (this.index >= this.iter.length)
            this.index = 0;
        return result;
    }

    static decrypt(exe, xorMethod) {
        const crc32 = function (hashKey, crcTable) {
            let result = 0xFFFFFFFF;
            for (const c of hashKey)
                result = ((result >>> 8) ^ crcTable[((result & 0xFF) ^ c) >>> 0]) >>> 0;
            return result;
        };
        const crc32Reflect = function (value, c) {
            let rValue = 0;
            for (let i = 1; i <= c; ++i) {
                if (((value & 1) >>> 0) != 0)
                    rValue = (rValue | (1 << (c - i))) >>> 0;
                value = value >>> 1;
